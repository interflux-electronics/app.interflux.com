{
  "name": "interflux",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for interflux goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "./node_modules/ember-cli/bin/ember build --environment production",
    "build-ios": "IOS_BUILD=true corber build --platform=ios --environment=production",
    "build-android": "ANDROID_BUILD=true corber build --platform=android --environment=production",
    "start": "./node_modules/ember-cli/bin/ember serve",
    "start-ios": "IOS_BUILD=true corber start --platform=ios",
    "start-android": "ANDROID_BUILD=true corber start --platform=android",
    "test": "./node_modules/ember-cli/bin/ember test",
    "deploy": "git checkout production -f; git pull origin master; git push; ./remote/deploy.sh; git checkout master",
    "bump-patch": "npm version patch; git push origin --tags; git push",
    "bump-minor": "npm version minor; git push origin --tags; git push",
    "bump-major": "npm version major; git push origin --tags; git push",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint ."
  },
  "devDependencies": {
    "@ember/optional-features": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "corber": "1.4.1",
    "corber-ember-livereload": "^0.0.2",
    "ember-cli": "~3.13.1",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.11.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^4.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "~3.13.0",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.7.0",
    "ember-get-config": "^0.2.4",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.3.0",
    "ember-source": "~3.13.0",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^10.0.0",
    "git-rev-sync": "^1.12.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0",
    "sass": "^1.23.3"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
}
