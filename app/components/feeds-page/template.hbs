<h1>Feed</h1>

<button {{on 'click' this.create}}>New</button>

{{#if @loading}}

{{else}}
  <ul>
    {{#each articlesSorted as |article|}}
      <FeedItem @article={{article}} />
    {{/each}}
  </ul>
{{/if}}